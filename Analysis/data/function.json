{
    "Pt": {
        "alias": "pt",

        "branch": "[P]_Pt",

        "axis-name": "p_{T}([P]) [GeV]",

        "hist-name": "Pt_[P]"
    },

    "Mass": {
        "alias": "m",

        "branch": "[P]_Mass",

        "axis-name": "m([P]) [GeV]",

        "hist-name": "Mass_[P]"
    },

    "Phi": {
        "alias": "phi",

        "branch": "[P]_Phi",

        "axis-name": "#phi([P]) [rad]",

        "hist-name": "Phi_[P]"
    },

    "Eta": {
        "alias": "eta",

        "branch": "[P]_Eta",

        "axis-name": "#eta([P]) [rad]",

        "hist-name": "Eta_[P]"
    },

    "Charge": {
        "alias": "charge",

        "branch": "[P]_Charge",

        "axis-name": "elec. Char([P]) [e]",

        "hist-name": "Charge_[P]"
    },

    "DiCharge": {
        "alias": "dicharge",

        "need": {
            "charge": [
                {"P1": 1},
                {"P2": 2}
            ]
        },

        "axis-name": "Charge([P]) * Charge([P])",

        "hist-name": "DiCharge_[P]_[P]"
    },

    "HT": {
        "alias": "HT",

        "axis-name": "H_{T} [GeV]",

        "need": {
            "pt": [
                {"P1": "1"},
                {"P2": "2"}
            ]

        },

        "hist-name": "HT"
    },

    "NParticle": {
        "alias": "N",

        "axis-name": "Number of [P]",

        "need": {
            "pt": [
                {"P": ""}
            ]

        },

        "hist-name": "N_[P]"
    },

    "dR": {
        "alias": "dR",

        "axis-name": "#Delta R([P], [P]) [rad]",

        "need": {
            "phi": [
                {"P1": 1},
                {"P2": 2}
            ],

            "eta": [
                {"P1": 1},
                {"P2": 2}
            ]
        },

        "hist-name": "dR_[P]_[P]"
    },

    "dPhi": {
        "alias": "dphi",

        "axis-name": "#Delta #phi([P], [P]) [rad]",

        "need": {
            "phi": [
                {"P1": 1},
                {"P2": 2}
            ]
        },

        "hist-name": "dPhi_[P]_[P]"
    },

    "LP": {
        "alias": "lp",

        "axis-name": "L_{P}",

        "need": {
            "pt": [
                {"P1": 1},
                {"P2": 2}
            ],

            "phi": [
                {"P1": 1},
                {"P2": 2}
            ]
        },

        "hist-name": "LP"
    },

    "Njettiness" : {
        "alias": "tau",

        "branch": "[P]_Njettiness[V]",

        "hist-name": "Njettiness[V]_[P]",

        "axis-name": "#tau_{[V]}([P])"
    },

    "DAK8" : {
        "alias": "dAK8",

        "branch":  "[P]_DeepAK8[V]",

        "hist-name": "[P]_DeepAK8[V]",

        "axis-name": "DeepAK8 score [V]"
    },

    "DAK8C" : {
        "alias": "dAK8C",

        "need": {
            "dAK8": [
                {"P1": 1, "V1": 1},
                {"P1": 1, "V2": 2},
                {"P1": 1, "V3": 3},
                {"P1": 1, "V4": 4},
                {"P1": 1, "V5": 5}
            ]
        },

        "hist-name": "[P]_DeepAK8C",

        "axis-name": "DeepAK8 class"
    },

    "DNN" : {
        "alias": "dnn",

        "branch": "DNN_[V][V]",
        "hist-name": "DNN_[V][V]",

        "axis-name": "DNN score [V] (m(H^{#pm}) = [V] GeV)"
    },

    "DNNC" : {
        "alias": "dnnc",

        "branch": "DNN_Class[V]",

        "hist-name": "DNNC_[V]",

        "axis-name": "DNN class (m(H^{#pm}) = [V] GeV)"
    }
}
    
