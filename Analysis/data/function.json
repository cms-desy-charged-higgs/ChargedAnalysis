{
    "Pt": {
        "alias": "pt",

        "branch": "[P]_Pt",

        "axis-name": "p_{T}([P]) [GeV]",

        "hist-name": "Pt_[P]"
    },

    "Mass": {
        "alias": "m",

        "branch": "[P]_Mass",

        "axis-name": "m([P]) [GeV]",

        "hist-name": "Mass_[P]"
    },

    "Phi": {
        "alias": "phi",

        "branch": "[P]_Phi",

        "axis-name": "#phi([P]) [rad]",

        "hist-name": "Phi_[P]"
    },

    "Eta": {
        "alias": "eta",

        "branch": "[P]_Eta",

        "axis-name": "#eta([P])",

        "hist-name": "Eta_[P]"
    },

    "Size": {
        "alias": "size",

        "branch": "[P]_Size",

        "axis-name": "N([P])",

        "hist-name": "Size_[P]"
    },


    "Charge": {
        "alias": "charge",

        "branch": "[P]_Charge",

        "axis-name": "elec. Char([P]) [e]",

        "hist-name": "Charge_[P]"
    },

    "Dxy": {
        "alias": "dxy",

        "branch": "[P]_Dxy",

        "axis-name": "d_{xy}([P]) [cm]",

        "hist-name": "Dxy_[P]"
    },

    "Dz": {
        "alias": "dz",

        "branch": "[P]_Dz",

        "axis-name": "d_{z}([P]) [cm]",

        "hist-name": "Dz_[P]"
    },

    "Isolation": {
        "alias": "iso",

        "branch": "[P]_Isolation",

        "axis-name": "iso.([P])",

        "hist-name": "Isolation_[P]"
    },

    "JetIso": {
        "alias": "jetiso",

        "branch": "[P]_JetRelIsolation",

        "axis-name": "rel. jet iso.",

        "hist-name": "JetIso_[P]"
    },

    "DiCharge": {
        "alias": "dicharge",

        "need": ["charge"],

        "axis-name": "Charge([P]) * Charge([P])",

        "hist-name": "DiCharge_[P]_[P]"
    },

    "DiMass": {
        "alias": "diM",

        "need": ["pt", "phi", "eta"],

        "axis-name": "m([P], [P]) [GeV]",

        "hist-name": "DiMass_[P]_[P]"
    },

    "DiMassTransverse": {
        "alias": "diMT",

        "need": ["pt", "phi"],

        "axis-name": "m_{T}([P], [P]) [GeV]",

        "hist-name": "DiMassTransverse_[P]_[P]"
    },

    "HT": {
        "alias": "HT",

        "axis-name": "H_{T} [GeV]",

        "need": ["pt"],

        "hist-name": "HT"
    },

    "NParticle": {
        "alias": "N",

        "axis-name": "N([P])",

        "need": ["size"],

        "hist-name": "N_[P]"
    },

    "dR": {
        "alias": "dR",

        "axis-name": "#Delta R([P], [P])",

        "need": ["phi", "eta"],

        "hist-name": "dR_[P]_[P]"
    },

    "dR3": {
        "alias": "dR3",

        "axis-name": "#Delta R([P], {[P], [P]})",

        "need": ["phi", "eta", "pt"],

        "hist-name": "dR_[P]_[P]_[P]"
    },

    "dPhi": {
        "alias": "dphi",

        "axis-name": "#Delta #phi([P], [P]) [rad]",

        "need": ["phi"],

        "hist-name": "dPhi_[P]_[P]"
    },

    "LP": {
        "alias": "LP",

        "axis-name": "L_{P}([P], [P])",

        "need": ["pt", "phi"],

        "hist-name": "LP_[P]_[P]"
    },

    "dPhi3": {
        "alias": "dphi3",

        "axis-name": "#Delta #phi([P], {[P], [P]}) [rad]",

        "need": ["phi", "eta", "pt"],

        "hist-name": "dPhi_[P]_[P]_[P]"
    },

    "isGenMatched": {
        "alias": "gM",

        "axis-name": "isGenMatched([P])",

        "need": ["gID"],

        "hist-name": "isGenMached_[P]"
    },

    "Njettiness" : {
        "alias": "tau",

        "branch": "[P]_Tau[V]",

        "hist-name": "Njettiness[V]_[P]",

        "axis-name": "#tau_{[V]}([P])"
    },

    "DAK8" : {
        "alias": "dAK8",

        "branch":  "[P]_DeepAK8[V]",

        "hist-name": "[P]_DeepAK8[V]",

        "axis-name": "DeepAK8 score [V]"
    },

    "DAK8C" : {
        "alias": "dAK8C",

        "branch": "[P]_DeepAK8ID",

        "hist-name": "[P]_DeepAK8C",

        "axis-name": "DeepAK8 class of [P]"
    },

    "DNN" : {
        "alias": "dnn",

        "branch": "DNN_[V]_[V]_[V]",
        "hist-name": "DNN_[V]_[V]",

        "axis-name": "DNN score [V] (m(H^{#pm}) = [V] GeV, m(h) = [V] GeV)"
    },

    "DNNC" : {
        "alias": "dnnc",

        "branch": "DNN_Class_[V]_[V]",

        "hist-name": "DNNC_[V]_[V]",

        "axis-name": "DNN class (m(H^{#pm}) = [V] GeV, m(h) = [V] GeV)"
    },

    "GenID" : {
        "alias": "gID",

        "branch": "[P]_GenID",

        "axis-name": "|PDG ID([P])|",

        "hist-name": "GenID"
    },

    "CutID" : {
        "alias": "ID",

        "branch": "[P]_CutID",

        "axis-name": "CutID([P]]",

        "hist-name": "CutID"
    },

    "MVAID" : {
        "alias": "mID",

        "branch": "[P]_MVAID",

        "axis-name": "MVAID",

        "hist-name": "MVAID"
    },

    "BTagID" : {
        "alias": "bjID",

        "branch": "[P]_DeepJetID",

        "axis-name": "BTagID",

        "hist-name": "BTagID"
    },

    "DeepAKID" : {
        "alias": "fjID",

        "branch": "[P]_DeepAK8ID",

        "axis-name": "BTagID",

        "hist-name": "BTagID"
    },

    "eventNumber" : {
        "alias": "evNr",

        "branch": "Misc_eventNumber",

        "axis-name": "event number",

        "hist-name": "eventNumber"
    },

    "modEventNumber" : {
        "alias": "mEvNr",

        "need": ["evNr"],

        "axis-name": "event number",

        "hist-name": "eventNumber"
    }
}
    
