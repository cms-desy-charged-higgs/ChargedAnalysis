{
    "Electron": {
        "alias": "e",

        "identification": {
            "Electron_ID": {
                "WP": {
                    "loose": {
                        "value": 1,
                        "compare": ">="
                    },

                    "medium": {
                        "value": 2,
                        "compare": ">="
                    },

                    "tight": {
                        "value": 3,
                        "compare": ">="
                    },

                    "veto": {
                        "value": 0,
                        "compare": ">="
                    }
                }
            },

            "Electron_Isolation": {
                "WP": {
                    "loose": {
                        "value": 0.25,
                        "compare": "<="
                    },

                    "medium": {
                        "value": 0.2,
                        "compare": "<="
                    },

                    "tight": {
                        "value": 0.15,
                        "compare": "<="
                    },

                    "veto": {
                        "value": 2,
                        "compare": ">="
                    }
                }
            }
        },    

        "scale-factors": {
            "EleReco": "Electron_recoSF[SHIFT]",
            "EleID": "Electron_[WP]SF[SHIFT]"
        },

        "axis-name": "e_{[I]}",

        "hist-name": "Electron_[I]_[WP]"
    },

    "Muon": {
        "alias": "mu",

        "identification": {
            "Muon_ID": {
                "WP": {
                    "loose": {
                        "value": 1,
                        "compare": ">="
                    },

                    "medium": {
                        "value": 2,
                        "compare": ">="
                    },

                    "tight": {
                        "value": 3,
                        "compare": ">="
                    }
                }
            },

            "Muon_isoID": {
                "WP": {
                    "loose": {
                        "value": 2,
                        "compare": ">="
                    },

                    "medium": {
                        "value": 2,
                        "compare": ">="
                    },

                    "tight": {
                        "value": 2,
                        "compare": ">="
                    }
                }
            }
        },

       "requirements": {
            "pt": {
                "compare": ">=",
                "cut": 30, 
                "particles": {
                    "THIS": ""
                }
            }
        },

        "scale-factors": {
            "MuonID": "Muon_[WP]SF[SHIFT]",
            "MuonTrigger": "Muon_triggerSF[SHIFT]",
            "MuonIso": "Muon_tightIso[WP]SF[SHIFT]"
        },

        "axis-name": "#mu_{[I]}",

        "hist-name": "Muon_[I]_[WP]"
    },

    "Jet": {
        "alias": "j",

       "requirements": {
            "dR": {
                "compare": ">=",
                "cut": 0.4, 
                "particles": {
                    "THIS": "",
                    "P2": {
                        "Muon": ["mu", 1, "medium"],
                        "Ele": ["e", 1, "medium"]
                    }
                }
            }
        },

        "axis-name": "j_{[I]}",

        "hist-name": "Jet_[I]"
    },

    "BJet": {
        "alias": "bj",

        "branch-prefix": "Jet",

        "identification": {
            "Jet_CSVScore": {
                "WP": {
                    "loose": {
                        "value": {
                            "2016": 0.2217,
                            "2017": 0.1522,
                            "2018": 0.1241
                        },
                        "compare": ">="
                    },

                    "medium": {
                        "value": {
                            "2016": 0.6321,
                            "2017": 0.4941,
                            "2018": 0.4184
                        },
                        "compare": ">="
                    },

                    "tight": {
                        "value": {
                            "2016": 0.8953,
                            "2017": 0.8001,
                            "2018": 0.7527
                        },
                        "compare": ">="
                    }
                }
            }
        },

       "requirements": {
            "dR": {
                "compare": ">=",
                "cut": 0.4, 
                "particles": {
                    "THIS": "",
                    "P2": {
                        "Muon": ["mu", 1, "medium"],
                        "Ele": ["e", 1, "medium"]
                    }
                }
            }
        },

        "scale-factors": {
            "BTag": "Jet_[WP]CSVbTagSF[SHIFT]"
        },

        "axis-name": "bj_{[I]}",

        "hist-name": "BJet_[I]_[WP]"
    },

    "FatJet": {
        "alias": "fj",

        "axis-name": "fj_{[I]}",

        "hist-name": "FatJet_[I]"
    },

    "SubJet": {
        "alias": "sj",

        "axis-name": "sj_{[I]}",

        "hist-name": "SubJet_[I]"
    },

    "MET": {
        "alias": "met",

        "axis-name": "#vec{p}_{T}^{miss}",

        "non-vector": true,

        "hist-name": "MET"
    },

    "IsoTrack": {
        "alias": "trk",

        "axis-name": "Iso. track [I]",

        "hist-name": "IsoTrack_[I]_[WP]",

        "identification": {
            "IsoTrack_PDG": {
                "absolute-values": true,
                "WP": {
                    "lep": {
                        "value": 13,
                        "compare": ">="
                    },

                    "QCD": {
                        "value": 1e8,
                        "compare": "<="
                    }
                }
            },

            "IsoTrack_Isolation": {
                "WP": {
                    "lep": {
                        "value": 0.2,
                        "compare": "<="
                    },

                    "QCD": {
                        "value": 2,
                        "compare": ">="
                    }
                }
            }
        },

        "requirements": {
            "dR": {
                "compare": ">=",
                "cut": 0.2,
                "particles": {
                    "THIS": "",
                    "P2": {
                        "Muon": ["mu", 1, "medium"],
                        "Ele": ["e", 1, "medium"]
                    }
                }
            },

            "dicharge": {
                "compare": "==",
                "cut": -1, 
                "particles": {
                    "THIS": "",
                    "P2": {
                        "Muon": ["mu", 1, "medium"],
                        "Ele": ["e", 1, "medium"]
                    }
                }
            }
        }
    },

    "HPlus": {
        "alias": "hc",

        "axis-name": "H^{#pm}",

        "hist-name": "HPlus",

        "non-vector": true
    },

    "W": {
        "alias": "W",

        "axis-name": "W^{#pm}",

        "hist-name": "W",

        "non-vector": true
    },

    "H1": {
        "alias": "h1",

        "axis-name": "h_{1}",

        "hist-name": "H1",

        "non-vector": true
    },

    "H2": {
        "alias": "h2",

        "axis-name": "h_{2}",

        "hist-name": "H2",

        "non-vector": true
    }
}
